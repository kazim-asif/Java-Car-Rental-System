                     <font>
                        <Font name="System Bold" size="21.0" />
                     </font>
                  </Label>
                  <Button layoutX="170.0" layoutY="346.0" mnemonicParsing="false" onAction="#guestLogin" prefHeight="31.0" prefWidth="135.0" text="Guest Login" textFill="#170202">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
               </children>
            </Pane>
         </children>
      </AnchorPane>
   </children>
